{
  "_meta": {
    "description": "Sample composable template that includes all ECS fields",
    "ecs_version": "1.12.1"
  },
  "composed_of": [
    "ecs_1.12.1_agent",
    "ecs_1.12.1_base",
    "ecs_1.12.1_client",
    "ecs_1.12.1_cloud",
    "ecs_1.12.1_container",
    "ecs_1.12.1_data_stream",
    "ecs_1.12.1_destination",
    "ecs_1.12.1_dll",
    "ecs_1.12.1_dns",
    "ecs_1.12.1_ecs",
    "ecs_1.12.1_error",
    "ecs_1.12.1_event",
    "ecs_1.12.1_file",
    "ecs_1.12.1_group",
    "ecs_1.12.1_host",
    "ecs_1.12.1_http",
    "ecs_1.12.1_log",
    "ecs_1.12.1_network",
    "ecs_1.12.1_observer",
    "ecs_1.12.1_orchestrator",
    "ecs_1.12.1_organization",
    "ecs_1.12.1_package",
    "ecs_1.12.1_process",
    "ecs_1.12.1_registry",
    "ecs_1.12.1_related",
    "ecs_1.12.1_rule",
    "ecs_1.12.1_server",
    "ecs_1.12.1_service",
    "ecs_1.12.1_source",
    "ecs_1.12.1_threat",
    "ecs_1.12.1_tls",
    "ecs_1.12.1_tracing",
    "ecs_1.12.1_url",
    "ecs_1.12.1_user",
    "ecs_1.12.1_user_agent",
    "ecs_1.12.1_vulnerability"
  ],
  "index_patterns": [
    "try-ecs-*"
  ],
  "priority": 1,
  "template": {
    "mappings": {
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ]
    },
    "settings": {
      "index": {
        "mapping": {
          "total_fields": {
            "limit": 2000
          }
        }
      }
    }
  }
}
{
  "_meta": {
    "description": "Sample composable template that includes all ECS fields",
    "ecs_version": "8.2.1"
  },
  "composed_of": [
    "ecs_8.2.1_base",
    "ecs_8.2.1_agent",
    "ecs_8.2.1_client",
    "ecs_8.2.1_cloud",
    "ecs_8.2.1_container",
    "ecs_8.2.1_data_stream",
    "ecs_8.2.1_destination",
    "ecs_8.2.1_dll",
    "ecs_8.2.1_dns",
    "ecs_8.2.1_ecs",
    "ecs_8.2.1_email",
    "ecs_8.2.1_error",
    "ecs_8.2.1_event",
    "ecs_8.2.1_faas",
    "ecs_8.2.1_file",
    "ecs_8.2.1_group",
    "ecs_8.2.1_host",
    "ecs_8.2.1_http",
    "ecs_8.2.1_log",
    "ecs_8.2.1_network",
    "ecs_8.2.1_observer",
    "ecs_8.2.1_orchestrator",
    "ecs_8.2.1_organization",
    "ecs_8.2.1_package",
    "ecs_8.2.1_process",
    "ecs_8.2.1_registry",
    "ecs_8.2.1_related",
    "ecs_8.2.1_rule",
    "ecs_8.2.1_server",
    "ecs_8.2.1_service",
    "ecs_8.2.1_source",
    "ecs_8.2.1_threat",
    "ecs_8.2.1_tls",
    "ecs_8.2.1_tracing",
    "ecs_8.2.1_url",
    "ecs_8.2.1_user_agent",
    "ecs_8.2.1_user",
    "ecs_8.2.1_vulnerability"
  ],
  "index_patterns": [
    "try-ecs-*"
  ],
  "priority": 1,
  "template": {
    "mappings": {
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ]
    },
    "settings": {
      "index": {
        "codec": "best_compression",
        "mapping": {
          "total_fields": {
            "limit": 2000
          }
        }
      }
    }
  }
}
